{
  "name": "sphtech/laravel-queue-aws-batch",
  "description": "Laravel Queue for AWS Batch, enabling users to submit jobs for processing to a Batch queue.",
  "keywords": [
    "laravel",
    "aws",
    "batch",
    "queue"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "SPHTech\\LaravelQueueAwsBatch\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SPHTech\\LaravelQueueAwsBatch\\": "tests/"
    }
  },
  "require": {
    "illuminate/bus": "~9.0",
    "illuminate/queue": "^9.0",
    "aws/aws-sdk-php": "^3.20.6",
    "ext-json": "*"
  },
  "require-dev": {
    "mockery/mockery": "^0.9.6",
    "laravel/framework": "~9.0",
    "phpunit/phpunit": "~8.2",
    "squizlabs/php_codesniffer": "3.*",
    "scrutinizer/ocular": "^1.3"
  },
  "scripts": {
    "cs": "phpcs --standard=psr2 src/",
    "test": "phpunit"
  }
}
